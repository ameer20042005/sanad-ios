{
  "expo": {
    "name": "سند",
    "slug": "sanad",
    "version": "1.0.2",
    "orientation": "default",
    "icon": "./assets/appLogo.png",
    "scheme": "sanad",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/appLogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "buildNumber": "6",
      "bundleIdentifier": "com.ameerwisam.sanad",
      "infoPlist": {
        "UIBackgroundModes": [
          "remote-notification"
        ],
        "ITSAppUsesNonExemptEncryption": false,
        "UIRequiredDeviceCapabilities": [
          "arm64"
        ],
        "UIRequiresFullScreen": false,
        "UISupportedInterfaceOrientations": [
          "UIInterfaceOrientationPortrait",
          "UIInterfaceOrientationPortraitUpsideDown",
          "UIInterfaceOrientationLandscapeLeft",
          "UIInterfaceOrientationLandscapeRight"
        ],
        "UISupportedInterfaceOrientations~ipad": [
          "UIInterfaceOrientationPortrait",
          "UIInterfaceOrientationPortraitUpsideDown",
          "UIInterfaceOrientationLandscapeLeft",
          "UIInterfaceOrientationLandscapeRight"
        ],
        "NSUserNotificationsUsageDescription": "نحتاج إلى صلاحية الإشعارات لإرسال تنبيهات فورية عن طلبات التبرع بالدم الجديدة",
        "NSCameraUsageDescription": "نحتاج إلى صلاحية الكاميرا لالتقاط الصور عند الحاجة"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/appLogo.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      "expo-localization",
      [
        "expo-notifications",
        {
          "icon": "./assets/appLogo.png",
          "color": "#DC2626",
          "sounds": [
            "./assets/notificationsound.mp3"
          ],
          "mode": "production"
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "نحتاج إلى صلاحية الكاميرا لالتقاط الصور عند الحاجة"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "aa130eea-4f2d-4185-b0a4-59f91e9c461a"
      }
    },
    "owner": "ameerwisam",
    "notification": {
      "icon": "./assets/appLogo.png",
      "color": "#DC2626"
    }
  }
}